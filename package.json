{
  "name": "blaze",
  "version": "1.0.0",
  "description": "Better Kindle comic recommender",
  "main": "src/igniter.js",
  "scripts": {
    "postinstall": "elm-package install --yes",
    "build": "npm run -s build:dev",
    "build:dev": "NODE_ENV=dev webpack",
    "build:prod": "NODE_ENV=prod webpack",
    "dynamo": "npm run -s dynamo:dev --",
    "dynamo:dev": "java -Djava.library.path=/opt/dynamodb_local/DynamoDBLocal_lib -jar /opt/dynamodb_local/DynamoDBLocal.jar -sharedDb",
    "clean": "rm -rf dist/",
    "clean:all": "rm -rf dist/ elm-stuff/ node_modules/",
    "package": "elm-package",
    "repl": "elm-repl",
    "test": "elm-test",
    "analyse": "elm-analyse"
  },
  "keywords": [
    "Elm",
    "Kindle",
    "Crawler"
  ],
  "author": "ymtszw",
  "license": "BSD-3-Clause",
  "repository": "https://github.com/ymtszw/blaze.git",
  "devDependencies": {
    "aws-sdk": "^2.135.0",
    "elm": "^0.18.0",
    "elm-analyse": "^0.12.1",
    "elm-test": "^0.18.9",
    "elm-webpack-loader": "^4.3.1",
    "eslint": "^4.8.0",
    "ini": "^1.3.4",
    "webpack": "^3.6.0",
    "xhr2": "^0.1.4"
  }
}
