{
  "name": "blaze",
  "version": "0.0.1",
  "description": "Better Kindle comic recommender",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "NODE_ENV=dev webpack",
    "build:prod": "NODE_ENV=prod webpack",
    "dynamo": "npm run dynamo:dev",
    "dynamo:dev": "java -Djava.library.path=/opt/dynamodb_local/DynamoDBLocal_lib -jar /opt/dynamodb_local/DynamoDBLocal.jar -sharedDb",
    "postcss": "npm run postcss:dev",
    "postcss:dev": "NODE_ENV=dev postcss src/blaze.css --output dist/blaze.css",
    "postcss:prod": "NODE_ENV=prod postcss src/blaze.css --output dist/blaze.min.css",
    "clean": "rm -rf dist/",
    "elm-package": "elm-package",
    "elm-repl": "elm-repl",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=dev webpack-dev-server",
    "start:prod": "NODE_ENV=prod webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Elm",
    "Kindle"
  ],
  "author": "ymtszw",
  "license": "MIT",
  "repository": "https://github.com/ymtszw/blaze.git",
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "elm": "^0.18.0",
    "elm-webpack-loader": "^4.3.1",
    "eslint": "^4.8.0",
    "file-loader": "^1.1.5",
    "postcss-cli": "^4.1.1",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.7",
    "precss": "^2.0.0",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-recommended": "^1.0.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
