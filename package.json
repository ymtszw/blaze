{
  "name": "blaze",
  "version": "0.0.1",
  "description": "Better Kindle comic recommender",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:dev --",
    "build:dev": "NODE_ENV=dev webpack",
    "build:prod": "NODE_ENV=prod webpack",
    "dynamo": "npm run dynamo:dev --",
    "dynamo:dev": "java -Djava.library.path=/opt/dynamodb_local/DynamoDBLocal_lib -jar /opt/dynamodb_local/DynamoDBLocal.jar -sharedDb",
    "clean": "rm -rf dist/",
    "elm-package": "elm-package",
    "elm-repl": "elm-repl",
    "test": "elm-test"
  },
  "keywords": [
    "Elm",
    "Kindle",
    "Crawler"
  ],
  "author": "ymtszw",
  "license": "MIT",
  "repository": "https://github.com/ymtszw/blaze.git",
  "devDependencies": {
    "aws-sdk": "^2.135.0",
    "elm": "^0.18.0",
    "elm-test": "^0.18.9",
    "elm-webpack-loader": "^4.3.1",
    "eslint": "^4.8.0",
    "ini": "^1.3.4",
    "webpack": "^3.6.0",
    "xhr2": "^0.1.4"
  }
}
